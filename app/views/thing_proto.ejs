<!doctype html>
<html class="no-js" lang="en">

<head>
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.3.3/angular.min.js"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/angular.js/1.3.3/angular-route.min.js"></script>
    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <link rel="stylesheet" href="https://code.getmdl.io/1.3.0/material.blue_grey-indigo.min.css">
    <script defer src="https://code.getmdl.io/1.3.0/material.min.js"></script>

    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>

    <script src="/socket.io/socket.io.js"></script>
    <script src="thing_proto.js"></script>

    <title>thing</title>
</head>

<body>
    
    <div class="mdl-layout mdl-js-layout ">
            <div class="mdl-grid">
            <!--card 1-->
                <div class="mdl-cell mdl-cell--4-col mdl-cell--4-col-tablet demo-card-wide mdl-card mdl-shadow--2dp">
                    <div class="mdl-card__title">
                        <h2 class="mdl-card__title-text">Chat</h2>
                    </div>
                    <div class="mdl-card__supporting-text">
                        <p id="demo"></p>
                        <ul id="messages"></ul>
                        <form action="">
                            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                <input id="m" autocomplete="off" class="mdl-textfield__input" type="text">
                                <label class="mdl-textfield__label" for="sample4">type a msg </label>
                            </div>
                            <button class="mdl-button mdl-js-button mdl-button--raised">
                                <i class="material-icons">trending_flat</i>
                            </button>
                        </form>
                    </div>
                    <div class="mdl-card__actions mdl-card--border">
                        <div id="teclado">
                            <button onclick="set_pin()" class="mdl-button mdl-js-button mdl-button--raised">
                                <i class="material-icons">input</i>
                            </button>
                            <button onclick="start_thing()" class="mdl-button mdl-js-button mdl-button--raised">
                                <i class="material-icons"> cached</i>
                            </button>
                        </div>
                    </div>
                    <div class="mdl-card__menu">
                        <button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect">
                            <i class="material-icons">visibility</i>
                        </button>
                    </div>
                </div>    
            <!--card keyboard-->
                <div class="mdl-cell mdl-cell--4-col mdl-cell--4-col-tablet demo-card-wide mdl-card mdl-shadow--2dp">
                    <div class="mdl-card__title">
                        <h2 class="mdl-card__title-text">keyboard</h2>
                    </div>
                    <div class="mdl-card__supporting-text">
                        <p id="demo_3" ></p>
                        <ul id="messages_3"></ul>
                        <form action="">
                            <div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
                                <input id="m_3" autocomplete="off" class="mdl-textfield__input" type="text">
                                <label class="mdl-textfield__label" for="sample4">type a msg 3</label>
                            </div>
                            <button onclick="addButtonPin()" class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab">
                                <i class="material-icons">add</i>
                            </button>
                        </form>
                    </div>
                    <div class="mdl-card__actions mdl-card--border">
                        <div id="teclado">
                            <button class="mdl-button mdl-js-button mdl-button--raised">
                                <i class="material-icons"> save</i>
                            </button>
                        </div>
                    </div>
                    <div class="mdl-card__menu">
                        <button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect">
                            <i class="material-icons">visibility</i>
                        </button>
                    </div>
                </div>
                
            <!--card 3-->
                <div class="mdl-cell mdl-cell--4-col mdl-cell--8-col-phone demo-card-wide mdl-card mdl-shadow--2dp">
                    <div class="mdl-card__title">
                        <h2 class="mdl-card__title-text">grafico</h2>
                    </div>
                    <div class="mdl-card__supporting-text">
                        <form action="">
                            <div id="chart_graf"></div>
                        </form>
                    </div>
                    <div class="mdl-card__actions mdl-card--border">
                        <div id="teclado">
                            <button onclick="start_grafic()" class="mdl-button mdl-js-button mdl-button--raised">
                                <i class="material-icons"> cached</i>
                            </button>
                        </div>
                    </div>
                    <div class="mdl-card__menu">
                        <button class="mdl-button mdl-button--icon mdl-js-button mdl-js-ripple-effect">
                            <i class="material-icons">visibility</i>
                        </button>
                    </div>
                </div>
                           
            </div>      
        
        <main class="mdl-layout__content">
            <footer class="mdl-mini-footer">
                <div class="mdl-mini-footer__left-section">
                    <div class="mdl-logo">Copyright Information</div>
                    <ul class=mdl-mini-footer__link-list"">
                        <li><a href="#">Help</a></li>
                        <li><a href="#">Privacy terms</a></li>
                        <li><a href="#">User Agreement</a></li>
                    </ul>
                </div>
                <div class="mdl-mini-footer__right-section">
                    <button class="mdl-mini-footer__social-btn">1</button>
                    <button class="mdl-mini-footer__social-btn">2</button>
                    <button class="mdl-mini-footer__social-btn">3</button>
                </div>
            </footer>
        </main>
    </div>
    
    
    <script src=" https://code.jquery.com/jquery-3.1.0.js "></script>
</body>

